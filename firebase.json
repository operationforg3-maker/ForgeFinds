{
  "firestore": {
    "rules": "firestore.rules"
  },
  "functions": {
    "source": "forgefinder",
    "runtime": "nodejs18",
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ]
  },
  "hosting": {
    "source": ".",
    "frameworksBackend": {
      "region": "us-central1"
    },
    "appAssociation": "AUTO"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
